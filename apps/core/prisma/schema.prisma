generator client {
    provider     = "prisma-client"
    output       = "../src/.generated/prisma-client"
    moduleFormat = "cjs"
}

generator zod {
    provider = "prisma-zod-generator"
    output   = "../src/.generated/zod"
    config   = "prisma/zod-generator.config.json"
}

datasource db {
    provider = "postgresql"
}
